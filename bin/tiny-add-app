#!/usr/bin/env python



def parse_args():
    import argparse

    parser = argparse.ArgumentParser(description='OpenStack Inventory Module')
    parser.add_argument('--action', action='store', help='Choose action')
    # parser.add_argument('')

    return parser.parse_args()


def main():
    args = parse_args()

    output = 'Nothing to return'
    print(args)


if __name__ == '__main__':
    main()
